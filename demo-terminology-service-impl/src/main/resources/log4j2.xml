<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <Console name="STDOUT" target="SYSTEM_OUT">
      <PatternLayout pattern="[%-5p] Thread-[%t] %C.%M() : %m%n"/>
    </Console>
  </Appenders>
  <loggers>
    <Logger name="org.apache.log4j.xml" level="WARN"/>
    <root level="WARN">
      <appender-ref ref="STDOUT"/>
    </root>
  </loggers>
</Configuration>